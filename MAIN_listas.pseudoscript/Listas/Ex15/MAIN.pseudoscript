ou


funcao tamanho(array):
	return array.tamanho()

funcao adicionarArray(array, resultado)
	para(i=0, i < tamanho(array), i++)
		resultado.adicionar(array[i])

array1 = [1, 2, 3]
array2 = [4, 5, 6]
resultado = []

adicionarArray(array1, resultado)
adicionarArray(array2, resultado)

para(i=0, i < tamanho(resultado), i++)
	imprime(resultado[i])


